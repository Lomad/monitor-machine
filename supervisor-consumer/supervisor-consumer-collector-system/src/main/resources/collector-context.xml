<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean class="com.winning.monitor.superisor.consumer.collector.DefaultCollectDataAnalyzeQueue">
        <constructor-arg index="0" value="10000"/>
        <constructor-arg index="1" ref="systemCollectDataAnalyzer"/>
    </bean>

    <bean id="systemCollectDataAnalyzer"
          class="com.winning.monitor.superisor.consumer.collector.system.SystemCollectDataAnalyzer"/>

</beans>